<fx:root hgap="7.0" type="GridPane" vgap="4.0" xmlns="http://javafx.com/javafx/8.0.102-ea"
         xmlns:fx="http://javafx.com/fxml/1">
    <Label text="Сервер Rserve:" GridPane.columnIndex="0" GridPane.rowIndex="0"/>
    <Label text="Пользователь Rserve:" GridPane.columnIndex="0" GridPane.rowIndex="1"/>
    <Label text="Пароль пользователя Rserve:" GridPane.columnIndex="0" GridPane.rowIndex="2"/>
    <Label text="Сервер Rapport:" GridPane.columnIndex="0" GridPane.rowIndex="3"/>
    <Label text="Администратор Rapport:" GridPane.columnIndex="0" GridPane.rowIndex="4"/>
    <Label text="Пароль администратора Rapport:" GridPane.columnIndex="0" GridPane.rowIndex="5"/>

    <TextField fx:id="rserveHostFld" GridPane.columnIndex="1" GridPane.columnSpan="2" GridPane.rowIndex="0"/>
